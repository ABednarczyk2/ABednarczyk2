# INT1PRC CHNAGE 
> #  lagged and differenced variables
> d7tr_mortgage <- d7tr_mortgage %>%
+   group_by(pid) %>%
+   mutate(
+     int1prc_lag = lag(int1prc),
+     int1prc_diff = int1prc - lag(int1prc)  # first difference
+   ) %>%
+   ungroup()
> # regression on the differenced dependent variable
